/%
	smem:
	.skip 512
%/
imply u8 smem;
#define SHELLMEM_SIZE 512
#import "stlfs16.dhulb"
int shell() {
	:shell__loop;
	print(username);
	print(":# ");
	readLine($$STDIN_FILENO$$, $$STDOUT_FILENO$$, smem$, $$SHELLMEM_SIZE$$);
	if (strcmp("exit", smem$) == 0) {
		return 0;
	}
	goto shell__loop;
	return 0;
}
